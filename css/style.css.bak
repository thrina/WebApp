

.header
{ 
	background: url(../images/banner.jpg);
	background-position: center center;
	-webkit-background-size: cover;
	-moz-background-size: cover;
	-o-background-size: cover;
	background-size: cover;
	min-height: 600px;
	width: 100%;
	position: relative;
}

   .navbar-nav>li>a
   {
    padding: 27px;
	color:#f9e1e1;
   }


.banner-section {
position: absolute; 
right: 0;
left: 0; 
top: 22%;
margin: 0 auto;
}
.banner-section h1 { 
color: white;text-align:center; 
font-size: 70px;
}

   .btn-findmore
    {
